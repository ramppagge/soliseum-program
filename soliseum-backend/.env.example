# Solana RPC (e.g. devnet or localnet)
SOLANA_RPC_URL=https://api.devnet.solana.com

# Oracle wallet private key (base58). NEVER commit the real key.
# Generate with: solana keypair new
ORACLE_PRIVATE_KEY=

# Server (API on PORT; Socket.io on SOCKET_PORT so GET / is not intercepted)
PORT=4000
SOCKET_PORT=4001

# Optional: log interval in ms for battle stream (500â€“1000)
BATTLE_LOG_INTERVAL_MS=700

# Phase 3: Database (Supabase/PostgreSQL)
DATABASE_URL=postgresql://user:pass@localhost:5432/soliseum

# Phase 3: Webhook secret (Helius/Shyft) - validate incoming webhook requests
WEBHOOK_SECRET=your-webhook-secret

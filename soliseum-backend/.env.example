# Solana RPC (e.g. devnet or localnet)
SOLANA_RPC_URL=https://api.devnet.solana.com

# Oracle wallet private key (base58). NEVER commit the real key.
# Generate with: solana keypair new
ORACLE_PRIVATE_KEY=

# Server - PORT for API. SOCKET_PORT: omit for single-port (deployment); set for local dev
PORT=4000
SOCKET_PORT=4001

# Optional: log interval in ms for battle stream (500â€“1000)
BATTLE_LOG_INTERVAL_MS=700

# Phase 3: Database (Supabase/PostgreSQL)
DATABASE_URL=postgresql://user:pass@localhost:5432/soliseum

# Phase 3: Helius Webhook (run: npm run setup:webhook)
HELIUS_API_KEY=
BACKEND_WEBHOOK_URL=https://your-backend.com
WEBHOOK_SECRET=your-webhook-secret

# CORS origin (use specific origin in production, e.g. https://soliseum.io)
CORS_ORIGIN=*

# Battle Engine: Agent API URLs for testing (leave empty to use MockAgent)
AGENT_A_API_URL=
AGENT_B_API_URL=
